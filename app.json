{
  "expo": {
    "name": "Zapi10",
    "slug": "mvt-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "projectId": "d4a3b53e-0dbc-48c1-a865-cf9eff2dd52c",
    "description": "Aplicativo de gerenciamento de entregas para motoristas. Receba notificações, visualize rotas e gerencie suas entregas com facilidade.",
    "githubUrl": "https://github.com/fabio1974/mvt-mobile",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f0f23"
    },
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Este app precisa acessar sua localização para otimizar as rotas de entrega e melhorar o serviço.",
        "NSLocationWhenInUseUsageDescription": "Este app precisa acessar sua localização para otimizar as rotas de entrega.",
        "UIBackgroundModes": [
          "location",
          "background-fetch",
          "remote-notification"
        ]
      },
      "config": {
        "googleMapsApiKey": "AIzaSyBpJ-PEX_eQunOFbDXKLC3Xr3q69xoROmU"
      },
      "bundleIdentifier": "com.mvt.mobile.zapi10"
    },
    "android": {
      "package": "com.mvt.mobile.zapi10",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f0f23"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBpJ-PEX_eQunOFbDXKLC3Xr3q69xoROmU"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#e94560",
      "androidMode": "default",
      "androidCollapsedTitle": "MVT Notifications",
      "vapidPublicKey": "BN8ym6-oByG6GoAEC9399ciwUoal_2vh1IyGph1xS9rnF0yC5GPHfrlgE-th3JqyPPaFOqXKnfQBYVj9oS5a15k",
      "serviceWorkerPath": "sw.js"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Este app precisa acessar sua localização para otimizar as rotas de entrega e melhorar o serviço.",
          "locationWhenInUsePermission": "Este app precisa acessar sua localização para otimizar as rotas de entrega.",
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/messaging"
    ],
    "extra": {
      "eas": {
        "projectId": "d4a3b53e-0dbc-48c1-a865-cf9eff2dd52c"
      }
    }
  }
}
